<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="AppModelBundle\Entity\NivelEducativoUsuarioRepository" name="AppModelBundle\Entity\NivelEducativoUsuario" table="nivel_educativo_usuario">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    
    <field name="fechaCreacion" type="datetime" column="fecha_creacion" nullable="0"/>
    <field name="anoFinalizacion" type="datetime" column="ano_finalizacion"/>
    
    <many-to-one field="carrera" target-entity="AppModelBundle\Entity\Carrera" inversed-by="nivelEducativos" orphan-removal="true">
        <join-column name="carrera_id" referenced-column-name="id" on-delete="restrict" on-update="cascade" nullable="0"/>
    </many-to-one>
    
    <many-to-one field="nivel" target-entity="AppModelBundle\Entity\NivelEducativo" inversed-by="nivelEducativos" orphan-removal="true">
        <join-column name="nivel_id" referenced-column-name="id" on-delete="restrict" on-update="cascade" nullable="0"/>
    </many-to-one>
    
    <many-to-one field="usuario" target-entity="AppModelBundle\Entity\Usuario" inversed-by="nivelEducativos" orphan-removal="true">
        <join-column name="usuario_id" referenced-column-name="id" on-delete="restrict" on-update="cascade" nullable="0"/>
    </many-to-one>
    
  </entity>
</doctrine-mapping>
