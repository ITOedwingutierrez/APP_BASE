<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="AppModelBundle\Entity\TarifaRepository" name="AppModelBundle\Entity\Tarifa" table="tarifa">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    
    <field name="fechaCreacion" type="datetime" column="fecha_creacion" nullable="0"/>
    <field name="precio" type="integer" column="precio" nullable="0"/>
    <field name="anoTrabajo" type="datetime" column="ano_trabajo" nullable="0"/>
    <field name="descripcion" type="text" column="descripcion" length="400" nullable="0"/>
    
    <many-to-one field="usuario" target-entity="AppModelBundle\Entity\User" inversed-by="tarifas" orphan-removal="true">
        <join-column name="usuario_id" referenced-column-name="id" on-delete="restrict" on-update="cascade" nullable="1"/>
    </many-to-one>
    
    <many-to-one field="tipoTrabajo" target-entity="AppModelBundle\Entity\TipoTrabajo" inversed-by="tarifas" orphan-removal="true">
        <join-column name="tipo_trabajo_id" referenced-column-name="id" on-delete="restrict" on-update="cascade" nullable="0"/>
    </many-to-one>
    
  </entity>
</doctrine-mapping>
